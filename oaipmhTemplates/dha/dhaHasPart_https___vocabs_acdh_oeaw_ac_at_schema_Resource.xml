<?xml version="1.0" encoding="UTF-8"?>
<dha:hasPart
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xml="http://www.w3.org/XML/1998/namespace"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:dha="https://digital-humanities.at/schema#"
	xmlns:premis="http://www.loc.gov/premis/rdf/v1#">
	<dha:RepoObject val="URI" asAttribute="rdf:about"
		xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
		<rdfs:label count="*" lang="true" val="/acdh:hasTitle"
			xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"/>
			<dha:hasMetadataDescription>
				<dha:MetadataDescription val="METAURL" asAttribute="rdf:about"
					xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
					<dc:title count="*" lang="true" val="/acdh:hasTitle"/>
					<dc:identifier count="*" val="/acdh:hasIdentifier"/>
					<dc:creator ComponentId="dhaDcCreator" count="*" val="@acdh:hasCreator"/>
					<dc:contributor ComponentId="dhaDcContributor" count="*" val="@acdh:hasContributor"/>
					<dc:rights count="*" lang="true" val="@acdh:hasLicense/acdh:hasTitle"/>
					<dc:language count="1" lang="true" val="@acdh:hasLanguage/acdh:hasTitle"/>
				</dha:MetadataDescription>
			</dha:hasMetadataDescription>
			<dha:hasPreservationDescription>
				<dha:PreservationDescription val="METAURL" asAttribute="rdf:about"
					xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
					<premis:hasIdentifier count="*" val="/acdh:hasPid"/>
					<dha:hasLicense count="*" val="/acdh:hasLicense" valueMap="license"/>
				</dha:PreservationDescription>
			</dha:hasPreservationDescription>
			<dha:hasAnnotationDescription>
				<dha:AnnotationDescription val="METAURL" asAttribute="rdf:about"
					xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
					<dha:hasAnnotationProperty ComponentId="dhaAnnotationConcept" val="/acdh:hasSubject" count="*"/>
					<dha:hasAnnotationProperty ComponentId="dhaAnnotationTimespan" val="/acdh:hasTemporalCoverageIdentifier" count="*"/>
					<dha:hasAnnotationProperty ComponentId="dhaAnnotationAgent" val="@acdh:hasActor" count="*"/>
					<dha:hasAnnotationProperty ComponentId="dhaAnnotationPlace" val="@acdh:hasSpatialCoverage" count="*"/>
				</dha:AnnotationDescription>
			</dha:hasAnnotationDescription>
			<dha:basedOn ComponentId="dhaBasedOn" val="@^acdh:isSourceOf" count="*"/>
			<dha:hasType count="*" val="@acdh:hasCategory/acdh:hasIdentifier" asAttribute="rdf:resource" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
		</dha:RepoObject>
	</dha:hasPart>
