<?xml version="1.0" encoding="UTF-8"?>
<!-- see https://guidelines.openaire.eu/en/latest/data/index.html, http://schema.datacite.org/meta/kernel-3/index.html -->
<resource xsi:schemaLocation="http://datacite.org/schema/kernel-3 http://schema.datacite.org/meta/kernel-3/metadata.xsd" xmlns="http://datacite.org/schema/kernel-3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!--INFO possible identifierTypes: ARK, DOI, Handle, PURL, URN, URL -->
    <identifier identifierType="Handle" val="OAIID"/>
    <creators>
        <creator ComponentId="creator" val="@acdh:hasCreator" count="*"/>
    </creators>
    <titles>
        <title val="/acdh:hasTitle" count="+" lang="true"/>
    </titles>
    <!--WARNING Cardinality in DataCite is "exact 1" and DataCite as well as OpenAIRE note that "this property will be used to formulate the citation, so consider the prominence of the role." -->
    <publisher val="@acdh:hasHosting/acdh:hasTitle" count="1"/>
    <publicationYear val="/acdh:hasAvailableDate" count="1" dateFormat="Y"/>
    <!--WARNING Check if right ARCHE property is used. It can be both free text or a controlled vocabulary (in the latter case it would be nice to also provide subjectScheme and schemeURI attributes)  -->
    <subjects>
        <subject val="@acdh:hasSubject" count="?" lang="true"/>
    </subjects>
    <contributors>
        <!--INFO possible contributorTypes: ContactPerson, DataCollector, DataCurator, DataManager, Distributor, Editor, Funder, HostingInstitution, Producer, ProjectLeader, ProjectManager, ProjectMember, RegistrationAgency, RegistrationAuthority, RelatedPerson, Researcher, ResearchGroup, RightsHolder, Sponsor, Supervisor, WorkPackageLeader, Other -->
        <contributor ComponentId="ContactPerson" val="@acdh:hasContact" count="*"/>
        <contributor ComponentId="DataCurator" val="@acdh:hasCurator" count="*"/>
        <contributor ComponentId="Editor" val="@acdh:hasEditor" count="*"/>
        <contributor ComponentId="HostingInstitution" val="@acdh:hasHosting" count="*"/>
        <contributor ComponentId="ProjectLeader" val="@acdh:hasPrincipalInvestigator" count="*"/>
        <contributor ComponentId="RightsHolder" val="@acdh:hasRightsHolder" count="*"/>
        <!--WARNING ARCHE schema suggests mapping acdh:hasFunder to datacite:contributor@contributorType=Sponsor but OpenAIRE requires datacite:contributor@contributorType=Funder to be provided, therefore mapping to it -->
        <contributor ComponentId="Funder" val="@acdh:hasFunder" count="*"/>
        <contributor ComponentId="Other" val="@acdh:hasContributor" count="*"/>
    </contributors>
    <dates>
        <!--INFO possible dateTypes: Accepted, Available, Copyrighted, Collected, Created, Issued, Submitted, Updated, Valid -->
        <date dateType="Accepted" val="/acdh:hasAcceptedDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Available" val="/acdh:hasAvailableDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Collected" val="/acdh:hasCollectedEndDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Created" val="/acdh:hasCreatedEndDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Issued" val="/acdh:hasAvailableDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Submitted" val="/acdh:hasSubmissionDate" dateFormat="Y-m-d" count="*"/>
        <date dateType="Updated" val="/acdh:hasUpdatedDate" dateFormat="Y-m-d" count="*"/>
    </dates>
    <!--WARNING In ARCHE 0-n in OpenAIRE 0-1 so we can't map it reliably -->
    <language val="@acdh:hasLanguage/acdh:hasTitle" count="?"/>
    <resourceType resourceTypeGeneral="Collection">Collection</resourceType>
    <alternateIdentifiers>
        <alternateIdentifier alternateIdentifierType="URL" val="/acdh:hasIdentifier" count="*"/>
    </alternateIdentifiers>
    <relatedIdentifiers>
        <!--INFO possible relationTypes: IsCitedBy, Cites, IsSupplementTo, IsSupplementedBy, IsContinuedBy, Continues, HasMetadata, IsMetadataFor, IsNewVersionOf, IsPreviousVersionOf, IsPartOf, HasPart, IsReferencedBy, References, IsDocumentedBy, Documents, IsCompiledBy, Compiles, IsVariantFormOf, IsOriginalFormOf, IsIdenticalTo, IsReviewedBy, Reviews, IsDerivedFrom, IsSourceOf
                 possible relatedIdentifierTypes ARK, arXiv, bibcode, DOI, EAN13, EISSN, Handle, ISBN, ISSN, ISTC, LISSN, LSID, PMID, PURL, UPC, URL, URN -->
        <!--WARNING Mapping below might be incomplete -->
        <relatedIdentifier relatedIdentifierType="Handle" relationType="IsNewVersionOf" val="@acdh:isNewVersionOf/acdh:hasPid" count="*"/>
        <relatedIdentifier relatedIdentifierType="Handle" relationType="IsPreviousVersionOf" val="@^acdh:isNewVersionOf/acdh:hasPid" count="*"/>
        <relatedIdentifier relatedIdentifierType="Handle" relationType="IsPartOf" val="@acdh:isPartOf/acdh:hasPid" count="*"/>
        <relatedIdentifier relatedIdentifierType="Handle" relationType="HasPart" val="@^acdh:isPartOf/acdh:hasPid" count="*"/>
        <!--WARNING This won't work for publications as ARCHE doesn't store (or even allow) acdh:hasPid on them -->
        <relatedIdentifier relatedIdentifierType="Handle" relationType="Documents" val="@acdh:documents/acdh:hasPid" count="*"/>
        <relatedIdentifier relatedIdentifierType="Handle" relationType="IsDocumentedBy" val="@^acdh:documents/acdh:hasPid" count="*"/>
    </relatedIdentifiers>
    <sizes>
        <size val="/acdh:hasExtent" count="*"/>
    </sizes>
    <!--WARNING ARCHE doesn't store format information on the collection level. Fortunately it's only optional in the datacite
    <formats>
        <format val="/acdh:" count="*"/>
    </formats>-->
    <version val="/acdh:hasVersion" count="*"/>
    <rightsList>
        <!--WARNING acdh:hasLicense is optional on the (Top)Collection. We are guaranteed to have acdh:hasLicenseSummary there but it's an open text (with counts) so it's pretty useless.  -->
        <rights ComponentId="rights" val="@acdh:hasLicense" count="*"/>
    </rightsList>
    <descriptions>
        <!--INFO possible descriptionType: Abstract, Methods, SeriesInformation, TableOfContents, Other -->
        <!--WARNING Check if we haven't missed any mapping -->
        <description descriptionType="Abstract" val="/acdh:description" count="*" lang="true"/>
        <description descriptionType="Methods" val="/acdh:hasAppliedMethod" count="*" lang="true"/>
        <description descriptionType="Methods" val="/acdh:hasAppliedMethodDescription" count="*" lang="true"/>
        <description descriptionType="SeriesInformation" val="/acdh:hasSeriesInformation" count="*" lang="true"/>
        <description descriptionType="TableOfContents" val="/acdh:hasTableOfContents" count="*" lang="true"/>
        <description descriptionType="Other" val="/acdh:hasTechnicalInfo" count="*" lang="true"/>
        <description descriptionType="Other" val="/acdh:hasNote" count="*" lang="true"/>
    </descriptions>
    <geoLocations>
        <geoLocation ComponentId="geoLocation" val="/acdh:hasSpatialCoverage" count="*"/>
    </geoLocations>
</resource>
