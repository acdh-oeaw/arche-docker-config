<?xml version="1.0" encoding="UTF-8"?>
<geoLocation xmlns="http://datacite.org/schema/kernel-3">
    <geoLocationPoint><latitude val="/acdh:hasLatitude" count="1" replaceXMLTag="true" match="(.*)" replace="\1 "/><longitude val="/acdh:hasLongitude" count="1" replaceXMLTag="true"/></geoLocationPoint>
    <!--We can't use geoLocationBox because we just have coordinates for the exact point-->
    <geoLocationPlace val="/acdh:hasTitle" count="1"/>
</geoLocation>
