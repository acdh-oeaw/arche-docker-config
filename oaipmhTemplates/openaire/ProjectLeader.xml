<?xml version="1.0" encoding="UTF-8"?>
<contributor contributorType="ProjectLeader" xmlns="http://datacite.org/schema/kernel-3">
    <contributorName val="/acdh:hasTitle" count="1"/>
    <!-- nameIdentifierScheme attribute is mandatory but can contain any value -->
    <nameIdentifier nameIdentifierScheme="ORCID" val="/acdh:hasIdentifier" match="^https://orcid[.]org/" count="*"/>
    <nameIdentifier nameIdentifierScheme="VIAF" val="/acdh:hasIdentifier" match="^https://viaf[.]org/" count="*"/>
    <nameIdentifier nameIdentifierScheme="GND" val="/acdh:hasIdentifier" match="^https://www[.]dnb[.]de/" count="*"/>
    <nameIdentifier nameIdentifierScheme="ARCHE" val="/acdh:hasIdentifier" match="^https://id[.]acdh[.]oeaw[.]ac[.]at/" count="*"/>
    <!--WARNING Can lead to many values differing only in language but datacite doesn't allow xml:lang on <affiliation>. Anyway, <affiliation> is 0-N so it's still syntactically valid. -->
    <affiliation val="@acdh:isMemberOf/acdh:hasTitle" count="*"/>
</contributor>

