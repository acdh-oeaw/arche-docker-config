<?xml version="1.0" encoding="UTF-8"?>
<HasView>
  <id val="OAIID" count="*"/>
  <url val="OAIID" format="image/jpeg"/>
  <title val="/acdh:hasTitle" count="*" lang="true"/>
  <ObjectType val="/acdh:hasCategory" count="*" valueMapProp="skos:relatedMatch" valueMapKeepSrc="true"/> <!-- not used for arche collection -->
  <Format val="/acdh:hasFormat" count="*"/> <!-- not used for arche collection -->
  <!-- iiif stuff -->
  <service>
    <url val="/acdh:hasIdentifier" match="https://id.acdh.oeaw.ac.at/(?!cmdi/|uuid/)(.*)$" replace="https://loris.acdh.oeaw.ac.at/uuid:/\1"/>
    <ConformsTo>http://iiif.io/api/image</ConformsTo>
    <Implements>http://iiif.io/api/image/2/level2.json</Implements>
  </service>
</HasView>

