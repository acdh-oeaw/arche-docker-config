# RAW
acdhids:raw
    a                       acdh:DisseminationService ;
    acdh:serviceLocation    "{RES_URI}" ;
    acdh:hasReturnType      "raw" ;
    acdh:serviceRevProxy    false ;
    acdh:hasTitle           "Download"@en ;
    acdh:hasDescription     "Download current file or collection"@en .

# Metadata
acdhids:rdf
    a                       acdh:DisseminationService ;
